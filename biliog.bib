@article{SolidStateCommun53,
	abstract = {The influence of the structural phase transition at Tc ⋍ 200 K on the electronic structure of 1T-TiSe2 is studied by means of high-resolution angle-resolved photoemission (ΔE = 70 meV; Δθ, Δπ = $\pm$0.5$\,^{\circ}$; ℏω = 21.2 eV) in the temperature regime 60 K ≦ T ≦ 300 K. At the Brillouin zone boundary at M(L) three peaks are found in the temperature dependent EDCs near EF whose intensity and binding energy vary continously, revealing the second order-type phase transition in the electronic properties accompanied by pretransition fluctuations. At low temperatures (T < Tc) these peaks are in good agreement with a recent band structure calculation of Suzuki et al., based on the band-type Jahn-Teller model, demonstrating the dominance of the electron-lattice interaction.},
	author = {O. Anderson and G. Karschnick and R. Manzke and M. Skibowski},
