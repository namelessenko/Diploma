@article{GREENAWAY19651445,
	abstract = {Single crystals of the following materials (all having the CdI2 crystal structure) have been prepared by iodine vapour transport: TiS2, TiSe2, TiTe2; ZrS2, ZrSe2; HfS2, HfSe2; and SnS2. These crystals were in platelet form, could all be readily cleaved, and had areas of up to 1 cm2. Measurements of optical absorption over the energy range 0.05 eV--2.5 eV showed that ZrS2, HfS2, HfSe2 and SnS2 were indirect gap semiconductors, (respective room temperature energy gaps: 1.68 eV, 1.96 eV, 1.13 eV and 2.21 eV). Measurements of reflectivity (energy range 1--12 eV) revealed a number of strong maxima in the joint density of states in the fundamental absorption region; from these results some tentative predictions about the energy band structure of these materials can be made. The measurements also reveal strong exciton effects deep in the fundamental absorption region of CdI2 itself. These results coupled with studies on the excitons in PbI2 and KBr support recent models put forward to explain the role of excitons and interband transitions in ionic crystals.},
	author = {D.L. Greenaway and R. Nitsche},
	doi = {https://doi.org/10.1016/0022-3697(65)90043-0},
	issn = {0022-3697},
	journal = {Journal of Physics and Chemistry of Solids},
	number = {9},
	pages = {1445-1458},
	title = {Preparation and optical properties of group IV--VI2 chalcogenides having the CdI2 structure},
	url = {https://www.sciencedirect.com/science/article/pii/0022369765900430},
	volume = {26},
	year = {1965},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/0022369765900430},
	bdsk-url-2 = {https://doi.org/10.1016/0022-3697(65)90043-0}}

@article{Benesh_1985,
	abstract = {The band structures of the layer compounds 1T-TiS2 and 1T-TiSe2 have been calculated within the local-density approximation using the self-consistent LAPW method. Both compounds are found to be semimetals with Gamma -L overlaps of 0.24 and 0.55 eV, respectively. The positioning and occupation of the bands near the Fermi level are not consistent with the carrier densities determined from Hall measurements; however, agreement is obtained when the overlap is decreased by about 0.25 eV. Calculations performed with contracted lattice constants reproduce qualitatively the same trends in Hall coefficients as observed experimentally. However, quantitative agreement cannot be obtained without an overlap decrease of about 0.25 eV-indicating that TiSe2 is a semimetal and TiS2 a semiconductor.},
	author = {G A Benesh and A M Woolley and C Umrigar},
	doi = {10.1088/0022-3719/18/8/007},
	journal = {Journal of Physics C: Solid State Physics},
	month = {mar},
	number = {8},
	pages = {1595--1603},
	publisher = {{IOP} Publishing},
	title = {The pressure dependences of TiS2 and TiSe2 band structures},
	url = {https://doi.org/10.1088/0022-3719/18/8/007},
	volume = {18},
	year = 1985,
	bdsk-url-1 = {https://doi.org/10.1088/0022-3719/18/8/007}}
	

@article{Beal_1972,
	abstract = {For pt. I see ibid., vol. 5, 3531 (1972). The transmission spectra of the layer type group VIA transition metal dichalcogenides have been measured at liquid helium temperature. Considerable sharpening of previously reported features have been found in addition to a great deal of new fine structure not reported in earlier work. The problem of two dimensional excitons has been discussed. The nature and origin of the observed features of the spectra and the trends which they follow from compound to compound have also been discussed. In the case of MoS2 the authors have attempted to assign and fit them in conjunction with other available data for this material to allowed transitions of a band structure.},
